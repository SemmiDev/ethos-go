            "gettingStarted": {
                "title": "Memulai",
                "q1": "Bagaimana cara membuat kebiasaan pertamaku?",
                "a1": "Klik tombol 'Buat Kebiasaan' di halaman Kebiasaan. Isi nama, deskripsi, frekuensi, dan target jumlah. Kamu juga bisa mengatur waktu pengingat harian untuk menerima notifikasi.",
                "q2": "Bisakah saya mengatur kebiasaan untuk hari tertentu dalam seminggu?",
                "a2": "Ya! Saat membuat kebiasaan, kamu bisa memilih frekuensi 'Mingguan' dan memilih hari tertentu dalam seminggu yang ingin kamu lacak. Ini cocok untuk kebiasaan seperti 'Gym di Senin, Rabu, Jumat'."
            },
            "tracking": {
                "title": "Pelacakan & Kemajuan",
                "q1": "Bagaimana cara mencatat penyelesaian kebiasaan?",
                "a1": "Klik kartu kebiasaan mana pun untuk melihat detail, lalu klik 'Log Kemajuan'. Kamu bisa mencatat beberapa penyelesaian jika targetmu lebih dari 1. Kamu juga bisa log cepat dari beranda.",
                "q2": "Apa arti warna-warna yang berbeda?",
                "a2": "Hijau menunjukkan kebiasaan yang selesai, kuning menunjukkan selesai sebagian (untuk kebiasaan dengan target), dan abu-abu mewakili kebiasaan yang belum dilacak hari ini. Merah menunjukkan kebiasaan yang terlewat dari hari sebelumnya."
            },
            "analytics": {
                "title": "Analitik & Wawasan",
                "q1": "Bagaimana tingkat penyelesaianku dihitung?",
                "a1": "Tingkat penyelesaianmu adalah persentase target kebiasaan yang telah kamu penuhi dalam periode tertentu. Untuk kebiasaan harian, dihitung setiap hari. Untuk kebiasaan mingguan, mempertimbangkan seluruh minggu.",
                "q2": "Bisakah saya mengekspor data kebiasaanku?",
                "a2": "Saat ini, ekspor data belum tersedia tapi ada di roadmap kami. Tetap update! Sementara itu, kamu bisa melihat statistik lengkap di halaman Analitik."
            },
            "account": {
                "title": "Akun & Keamanan",
                "q1": "Bagaimana cara mengubah kata sandiku?",
                "a1": "Buka Pengaturan > Keamanan dan klik 'Ubah Kata Sandi'. Kamu perlu memasukkan kata sandi saat ini lalu kata sandi baru dua kali untuk konfirmasi.",
                "q2": "Bagaimana cara mengelola sesi aktifku?",
                "a2": "Di Pengaturan > Sesi Aktif, kamu bisa melihat semua perangkat yang sedang login. Kamu bisa mencabut akses dari perangkat mana pun atau keluar dari semua perangkat sekaligus untuk keamanan."
            },
            "notifications": {
                "title": "Notifikasi",
                "q1": "Bagaimana cara mengatur pengingat?",
                "a1": "Saat membuat atau mengedit kebiasaan, atur waktu 'Pengingat Harian'. Kamu akan menerima notifikasi pada waktu tersebut untuk mengingatkanmu menyelesaikan kebiasaanmu.",
                "q2": "Bisakah saya menyesuaikan waktu notifikasi?",
                "a2": "Ya, kamu bisa mengatur waktu pengingat khusus untuk setiap kebiasaan. Buka halaman detail kebiasaan dan konfigurasikan jadwal notifikasi yang paling cocok untukmu."
            }
        }
